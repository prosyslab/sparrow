(rule
 (deps alignof0.c)
 (targets alignof0.output)
 (action
  (with-stdout-to
   %{targets}
   (pipe-stdout
    (ignore-stderr
     (run
      ../../src/main.exe
      -verbose
      0
      -frontend
      claml
      -outdir
      sparrow-out-%{deps}
      -il
      %{deps}))
    (run clang-format)))))

(rule
 (alias runtest)
 (action
  (diff alignof0.expected alignof0.output)))

