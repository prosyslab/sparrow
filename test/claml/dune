 (targets label1.output)
 (action
  (with-stdout-to
   %{targets}
   (pipe-stdout
    (ignore-stderr
     (run
      ../../src/main.exe
      -verbose
      0
      -frontend
      claml
      -outdir
      sparrow-out-%{deps}
      -il
      %{deps}))
    (run clang-format)))))

(rule
 (alias runtest)
 (action
  (diff label1.expected label1.output)))

